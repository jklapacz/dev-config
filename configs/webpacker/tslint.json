{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rules": {
      "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"],
      "no-implicit-dependencies": [true, "dev"],
      "no-submodule-imports": [true, "@pindrop/iris"],
      "no-console": [false, "log", "error"]
  }
}
